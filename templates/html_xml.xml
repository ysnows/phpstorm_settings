<templateSet group="html/xml">
  <template name="navheader" value="nav class=&quot;top1 navbar navbar-default navbar-static-top&quot; role=&quot;navigation&quot;&#10;         style=&quot;margin-bottom: 0&quot;&gt;&#10;        &lt;include file='nav_header'/&gt;&#10;        &lt;include file='tabs'/&gt;&#10;        &lt;include file='menu'/&gt;&#10;    &lt;/nav&gt;&#10;" description="header" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="copyright" value="include file='copyright'/&gt;&#10;" description="copyright" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="u" value="{:U('$url$')}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="url" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="dcl" value="div class=&quot;$class$&quot;&gt;&#10;    $content$&#10;&lt;/div&gt;" description="div with class" toReformat="true" toShortenFQNames="true">
    <variable name="class" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="VUE_COMPONENT_DESCRIPTOR" value="true" />
    </context>
  </template>
  <template name="icla" value="i class=&quot;$class$&quot;&gt;&lt;/i&gt;&#10;" description="i with class" toReformat="true" toShortenFQNames="true">
    <variable name="class" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="VUE_COMPONENT_DESCRIPTOR" value="true" />
    </context>
  </template>
  <template name="scl" value="span class=&quot;$class$&quot;&gt;$content$&lt;/span&gt;&#10;" description="i with class" toReformat="true" toShortenFQNames="true">
    <variable name="class" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="VUE_COMPONENT_DESCRIPTOR" value="true" />
    </context>
  </template>
</templateSet>